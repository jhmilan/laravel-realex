{
    "name": "square1/laravel-realex",
    "description": "A wrapper for shaneog/realex in Laravel",
    "authors": [
        {
            "name": "Square1 Ltd.",
            "email": "info@square1.io"
        }
    ],
    "repositories": [
        {
            "type": "vcs",
            "url": "https://bitbucket.org/square1/realex-payments"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "illuminate/support": "4.2.*",
        "shaneog/realex": "1.1@dev"
